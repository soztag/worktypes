@article{YanoMeasuringHappinessUsing2015a,
  title = {Measuring {{Happiness Using Wearable Technology}}},
  volume = {64},
  abstract = {Instead of the industrial production, which drove economic growth in the 20th century, the driver for growth in the 21st century is productivity in knowledge work and service businesses. For this reason, the authors developed a technique for measuring happiness using wearable technology. The research found a hidden signal representing a person's happiness within the basic pattern of physical activity known as the ``1/T rule.'' This result uncovered a close relationship between the ``trinity'' of physical activity = happiness = productivity. Combined with a technique for using artificial intelligence to generate KPIs automatically, the technology can foster the ability of groups to engage in ``co-creation'' while also encouraging autonomy and commitment in individuals. This new technology is recognized as having the potential to open up new pathways in corporate accounting, production, and human resource systems.},
  number = {8},
  journal = {Hitachi Review},
  author = {Yano, Kazuo and Akitomi, Eng Tomoaki and Ara, Koji and Watanabe, Eng Junichiro and Tsuji, Eng Satomi and Sato, Nobuo and Hayakawa, Miki},

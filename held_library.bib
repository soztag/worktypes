  pages = {593--9},
  file = {/Users/max/zotero-library/storage/95FU4M7R/Eikemo_et_al-2008-593-9-Wel.pdf},
  class = {Welfare States  Health Wellbeing 2010, Diss Tax  Democracy 2010ff},
  date-modified = {2014-06-14 15:13:01 +0000},
  reading = {Mandatory},
  bdsk-url-1 = {http://www.ncbi.nlm.nih.gov/pubmed/18927186},
